{
  "name": "laravel-mix-boilerplate",
  "version": "2.0.0",
  "description": "Standalone Laravel Mix boilerplate",
  "repository": "tomothumb/laravel-mix-boilerplate",
  "keywords": [
    "laravel",
    "webpack",
    "laravel mix"
  ],
  "author": "Tomoyuki Tsujimoto",
  "license": "MIT",
  "scripts": {
    "dev": "npm run development",
    "development": "mix",
    "watch": "mix watch",
    "watch-poll": "mix watch -- --watch-options-poll=1000",
    "hot": "mix watch --hot",
    "prod": "npm run production",
    "production": "mix --production"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.18.6",
    "laravel-mix": "^6.0.49",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.54.4",
    "sass-loader": "^12.6.0",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
