{
  "name": "laravel-mix-boilerplate",
  "version": "2.0.0",
  "description": "Standalone Laravel Mix boilerplate",
  "repository": "tomothumb/laravel-mix-boilerplate",
  "keywords": [
    "laravel",
    "webpack",
    "laravel mix"
  ],
  "author": "Tomoyuki Tsujimoto",
  "license": "MIT",
  "scripts": {
    "dev": "npm run development",
    "development": "mix",
    "watch": "mix watch",
    "watch-poll": "mix watch -- --watch-options-poll=1000",
    "hot": "mix watch --hot",
    "prod": "npm run production",
    "production": "mix --production"
  },
  "devDependencies": {
    "laravel-mix": "^6.0.49"
  }
}
